<div class="author">
<h1 class="author_heading">Search Results</h1>

</div>

<section class="articles">
<ul class="article-ul">
 <% data.foreach(post => { %> 
<li>
    <a href="/post/<%- post_id %>">
        <span><%- post.title %> </span>
        <span class="article-list_date"><%- post.createdAt.toDateString() %></span>
    </a>
</li>

   <% }) %>

</ul>


</section>